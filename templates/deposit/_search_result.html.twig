<div class="list-group">
    {% for id in ids %}
        <div
                class="list-group-item list-group-item-action"
                data-action="click->deposit-lookup#queryDepo"
                data-deposit-lookup-target="resultItem"
                data-id="{{ id.id }}"
        >
            {{ id.document }}
        </div>
    {% else %}
        <div class="list-group-item">
            {{ 'No results found :('|trans }}
        </div>
    {% endfor %}
</div>
