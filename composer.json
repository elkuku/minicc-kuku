{
    "name": "elkuku/minicc-kuku",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "knplabs/knp-snappy",
                "version": "1.5.99",
                "dist": {
                    "url": "https://api.github.com/repos/KnpLabs/snappy/zipball/3dd138e9e47de91cd2e056c5e6e1a0dd72547ee7",
                    "type": "zip"
                },
                "autoload": {
                    "psr-4": {
                        "Knp\\Snappy\\": "src/Knp/Snappy"
                    }
                },
                "require": {
                    "php": ">=8.1",
                    "symfony/process": "^5.0||^6.0||^7.0||^8.0",
                    "psr/log": "^2.0||^3.0"
                }
            }
        }
    ],
    "description": "Mini CC Manager",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.3",
        "elkuku/intl-numbers-to-words": "1.*",
        "elkuku/kuku-sf-system-tools-bundle": "^1.0",
        "google/apiclient": "^2.13",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "knplabs/knp-snappy": "^1.5",
        "knplabs/knp-snappy-bundle": "dev-master",
        "oro/doctrine-extensions": "^3.0-beta1",
        "symfony/apache-pack": "*",
        "symfony/asset": "*",
        "symfony/asset-mapper": "8.*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/google-mailer": "*",
        "symfony/mailer": "8.*",
        "symfony/monolog-bundle": "^4.0",
        "symfony/process": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/sendgrid-mailer": "*",
        "symfony/serializer": "*",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/ux-chartjs": "^2.0",
        "symfony/ux-icons": "^2.18",
        "symfony/ux-twig-component": "^2.18",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "twig/cssinliner-extra": "^3.19",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "^3.20",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.5",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "elkuku/symfony-utils": "dev-master",
        "jawira/doctrine-diagram-bundle": "dev-main",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^2",
        "phpunit/phpunit": "^13",
        "rector/rector": "^2",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.29",
        "symfony/phpunit-bridge": "^8.0",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*",
        "zenstruck/foundry": "^2.9"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test-phpunit": ["make tests"],
        "test-phpstan": ["vendor/bin/phpstan analyse --memory-limit=2G"],
        "ci": ["@test-phpunit", "@test-phpstan"],
        "coverage": ["APP_ENV=test symfony console doctrine:database:drop --force || true", "APP_ENV=test symfony console doctrine:database:create", "XDEBUG_MODE=coverage php vendor/bin/phpunit --coverage-text"],
        "coverage-html": ["APP_ENV=test symfony console doctrine:database:drop --force || true", "APP_ENV=test symfony console doctrine:database:create", "XDEBUG_MODE=coverage php vendor/bin/phpunit --coverage-html coverage"],
        "check" : [
            "echo 'Check for outdated assets:'",
            "bin/console importmap:outdated",
            "echo 'Check for outdated PHP deps:'",
            "composer outdated",
            "echo 'Check for outdated Recipes:'",
            "composer recipes:update"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "8.*",
            "docker": true
        }
    }
}
