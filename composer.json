{
    "name": "elkuku/minicc-kuku",
    "description": "Mini CC Manager",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "easycorp/easyadmin-bundle": "^4.0",
        "elkuku/intl-numbers-to-words": "1.*",
        "google/apiclient": "^2.13",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "knplabs/knp-snappy-bundle": "^1.10",
        "oro/doctrine-extensions": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/apache-pack": "*",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/google-mailer": "*",
        "symfony/mailgun-mailer": "7.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/sendgrid-mailer": "*",
        "symfony/serializer": "*",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/ux-chartjs": "^2.0",
        "symfony/ux-twig-component": "^2.9",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "*",
        "symfony/yaml": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.5",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.0",
        "elkuku/symfony-utils": "dev-master",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^10",
        "rector/rector": "^0.18",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.29",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*",
        "tomasvotruba/cognitive-complexity": "^0.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "trans": [
            "./bin/console translation:extract es --format=yaml --force"
        ],
        "test-phpunit": ["make tests"],
        "test-phpstan": ["vendor/bin/phpstan analyse"],
        "ci": ["@test-phpunit", "@test-phpstan"]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.*",
            "docker": true
        }
    }
}
