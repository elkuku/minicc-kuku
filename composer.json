{
  "name": "elkuku/minicc-kuku",
  "description": "Mini CC Manager",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pcre": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "elkuku/g11n-bundle": "^0.1.2",
    "elkuku/intl-numbers-to-words": "1.0",
    "h4cc/wkhtmltopdf-amd64": "^0.12.3",
    "knplabs/knp-snappy-bundle": "^1.5",
    "oro/doctrine-extensions": "^1.2",
    "sensio/framework-extra-bundle": "^5.1",
    "sensiolabs/security-checker": "^5.0",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "^1.1",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/security-bundle": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/webpack-encore-bundle": "dev-master",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "joomla/coding-standards": "dev-master",
    "phpstan/phpstan": "^0.10.3",
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "*",
    "symfony/css-selector": "*",
    "symfony/debug-bundle": "*",
    "symfony/dotenv": "*",
    "symfony/maker-bundle": "^1.5",
    "symfony/phpunit-bridge": "*",
    "symfony/web-profiler-bundle": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": ["vendor/elkuku/g11n/src/Language/methods.php"]

  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpcs": "vendor/bin/phpcs src --standard=ruleset.xml",
    "test": "bin/phpunit",
    "tpl":"bin/console g11n:templates",
    "lang":"bin/console g11n:langfiles de-DE es-ES",
    "ci": [
      "@composer validate --no-check-all --strict",
      "@phpcs",
      "@test"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01BZWDWDTYCF0SM409YTJTXP3E",
      "allow-contrib": true,
      "require": "4.3.*"
    }
  }
}
