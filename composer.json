{
    "name": "elkuku/minicc-kuku",
    "description": "Mini CC Manager",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "elkuku/intl-numbers-to-words": "1.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12.3",
        "knplabs/knp-snappy-bundle": "^1.5",
        "knpuniversity/oauth2-client-bundle": "^v2.1",
        "league/oauth2-google": "^3.0",
        "oro/doctrine-extensions": "^2.0",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/apache-pack": "*",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/google-mailer": "*",
        "symfony/mailgun-mailer": "5.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/runtime": "5.*",
        "symfony/security-bundle": "*",
        "symfony/sendgrid-mailer": "5.*",
        "symfony/serializer-pack": "*",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-pack": "*",
        "symfony/ux-chartjs": "^1.1",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^0.3.0",
        "rector/rector": "^0.11.7",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "5.*",
        "symfony/css-selector": "*",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.29",
        "symfony/panther": "^1.0",
        "symfony/test-pack": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpcs": "vendor/bin/phpcs src --standard=ruleset.xml",
        "test": "bin/phpunit",
        "ci": [
            "@composer validate --no-check-all --strict",
            "@phpcs",
            "@test"
        ],
        "trans": [
            "./bin/console translation:update es --output-format=yaml --force"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.*"
        }
    }
}
