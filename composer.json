{
  "name": "MiniCC-KuKu",
  "description": "Mini CC Manager",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "elkuku/intl-numbers-to-words": "dev-master",
    "h4cc/wkhtmltopdf-amd64": "^0.12.3",
    "knplabs/knp-snappy-bundle": "dev-master",
    "oro/doctrine-extensions": "^1.2",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "^4.1",
    "symfony/console": "^4.1",
    "symfony/expression-language": "^4.1",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/lts": "^4@dev",
    "symfony/security-bundle": "^4.1",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/twig-bundle": "^4.1",
    "symfony/validator": "^4.1",
    "symfony/webpack-encore-pack": "*",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "joomla/coding-standards": "~2.0@alpha",
    "symfony/debug-pack": "*",
    "symfony/dotenv": "^4.1",
    "symfony/maker-bundle": "^1.5",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpcs": "vendor/bin/phpcs src --standard=ruleset.xml"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01BZWDWDTYCF0SM409YTJTXP3E",
      "allow-contrib": false
    }
  }
}
