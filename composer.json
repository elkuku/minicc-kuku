{
  "name": "elkuku/minicc-kuku",
  "description": "Mini CC Manager",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pcre": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "elkuku/intl-numbers-to-words": "1.0",
    "h4cc/wkhtmltopdf-amd64": "^0.12.3",
    "knplabs/knp-snappy-bundle": "^1.5",
    "oro/doctrine-extensions": "^1.2",
    "sensio/framework-extra-bundle": "^5.1",

    "symfony/apache-pack": "*",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "*",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/google-mailer": "*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/orm-pack": "*",
    "symfony/process": "*",
    "symfony/serializer-pack": "*",
    "symfony/string": "*",
    "symfony/translation": "*",
    "symfony/security-bundle": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/twig-pack": "*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/webpack-encore-bundle": "dev-master",
    "symfony/yaml": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "*",
    "symfony/css-selector": "*",
    "symfony/debug-pack": "*",
    "symfony/dotenv": "*",
    "symfony/maker-bundle": "^1.5",
    "symfony/test-pack": "*",
    "symfony/web-profiler-bundle": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpcs": "vendor/bin/phpcs src --standard=ruleset.xml",
    "test": "bin/phpunit",
    "ci": [
      "@composer validate --no-check-all --strict",
      "@phpcs",
      "@test"
    ],
    "trans": [
      "./bin/console translation:update es --output-format=yaml --force"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.*"
    }
  }
}
